<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/result_main"
        layout="@layout/result_main"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_h1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.89"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_h2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.78" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_h3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.67" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_h4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.56" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_h5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.45" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_v1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_v2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.50" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_v3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75"
        />
    


    <Button
        android:id="@+id/ac_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button1"
        android:text="AC"
        android:textSize="18dp"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintEnd_toEndOf="@id/guideline_v1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h5"
        app:layout_constraintBottom_toBottomOf="@+id/guideline_h4"/>

    <Button
        android:id="@+id/persen_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="%"
        android:textColor="#FF8F00"
        android:textSize="25dp"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@+id/guideline_v1"
        app:layout_constraintEnd_toEndOf="@id/guideline_v2"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h4"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h5" />

    <Button
        android:id="@+id/bagi_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="/"
        android:textSize="25dp"
        android:textColor="#FF8F00"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v2"
        app:layout_constraintEnd_toEndOf="@id/guideline_v3"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h5"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h4"/>

    <Button
        android:id="@+id/kali_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="X"
        android:textSize="25dp"
        android:textColor="#FF8F00"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h5"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h4"/>

    <Button
        android:id="@+id/btn7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="7"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/guideline_v1"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h4"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h3"/>

    <Button
        android:id="@+id/btn8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="8"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v1"
        app:layout_constraintEnd_toEndOf="@id/guideline_v2"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h4"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h3"/>

    <Button
        android:id="@+id/btn9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="9"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v2"
        app:layout_constraintEnd_toEndOf="@id/guideline_v3"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h4"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h3"/>

    <Button
        android:id="@+id/tambah_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="+"
        android:textSize="25dp"
        android:textColor="#FF8F00"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h4"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h3"/>

    <Button
        android:id="@+id/btn4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="4"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/guideline_v1"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h3"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h2"/>

    <Button
        android:id="@+id/btn5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="5"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v1"
        app:layout_constraintEnd_toEndOf="@id/guideline_v2"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h3"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h2"/>

    <Button
        android:id="@+id/btn6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="6"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v2"
        app:layout_constraintEnd_toEndOf="@id/guideline_v3"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h3"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h2"/>

    <Button
        android:id="@+id/kurang_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="-"
        android:textSize="25dp"
        android:textColor="#FF8F00"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h3"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h2"/>

    <Button
        android:id="@+id/btn1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="1"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/guideline_v1"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h2"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h1"/>

    <Button
        android:id="@+id/btn2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="2"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v1"
        app:layout_constraintEnd_toEndOf="@id/guideline_v2"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h2"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h1"/>

    <Button
        android:id="@+id/btn3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="3"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v2"
        app:layout_constraintEnd_toEndOf="@id/guideline_v3"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h2"
        app:layout_constraintBottom_toBottomOf="@id/guideline_h1"/>

    <Button
        android:id="@+id/hasil_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button1"
        android:text="="
        android:textSize="25dp"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h2"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <Button
        android:id="@+id/btn0"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="0"
        android:textSize="25dp"
        android:textColor="@color/black"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/guideline_v2"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h1"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <Button
        android:id="@+id/titik_btn"
        android:layout_width="85dp"
        android:layout_height="64dp"
        android:layout_margin="8dp"
        android:background="@drawable/button2"
        android:text="."
        android:textSize="25dp"
        android:textColor="#FF8F00"
        app:backgroundTint="@null"
        android:elevation="10dp"
        android:translationZ="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline_v2"
        app:layout_constraintEnd_toEndOf="@id/guideline_v3"
        app:layout_constraintTop_toBottomOf="@+id/guideline_h1"
        app:layout_constraintBottom_toBottomOf="parent"/>



</androidx.constraintlayout.widget.ConstraintLayout>